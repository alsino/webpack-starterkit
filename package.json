{
  "name": "slim-webpack-starterkit",
  "version": "0.0.1",
  "description": "Slim Webpack Starterkit with ES6, SCSS, Jade, HMR, Normalize, PureCSS",
  "scripts": {
    "build": "webpack --config webpack-prod.config.js --colors",
    "dev": "webpack-dev-server --config webpack-dev.config.js --watch --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+github.com:z3to/webpack-starterkit.git"
  },
  "keywords": [
    "webpack",
    "startkit",
    "frontend",
    "es6",
    "javascript",
    "webdev",
    "jade",
    "scss"
  ],
  "author": "Jonas Parnow",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/z3to/webpack-starterkit/issues"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "rules": {
      "semi": 2
    }
  },
  "homepage": "https://github.com/z3to/webpack-starterkit#readme",
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.5.0",
    "css-loader": "^0.23.1",
    "html-loader": "^0.4.3",
    "jade-loader": "^0.8.0",
    "eslint": "^2.0.0",
    "eslint-loader": "^1.2.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.8.1",
    "node-sass": "^3.4.2",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1",
    "jade": "^1.11.0",
    "purecss": "^0.6.0",
    "normalize.css": "^3.0.3"
  },
  "dependencies": {
    "babel-polyfill": "^6.5.0"
  }
}
